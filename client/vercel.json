{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      { "src": "/data/(.*)", "dest": "http://localhost:4000/$1" },
      { "handle": "filesystem" },
      { "src": ".*", "dest": "/index.html" }
    ]
  }
  