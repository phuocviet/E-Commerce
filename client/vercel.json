{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "buildCommand": "npm run start:json-server && next build",
          "devCommand": "npm run start:json-server && next dev",
          "outputDirectory": "out"
        }
      }
    ],
    
    "routes": [
      {
        "src": "/api/.*",
        "methods": ["GET", "POST", "PUT", "DELETE"],
        "dest": "api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  