{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run build",
        "devCommand": "npm run dev",
        "outputDirectory": "out"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "dest": "http://localhost:4000/"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
